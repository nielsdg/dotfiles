  'ssid="eduroam"'
  'key_mgmt=WPA-EAP'
  'eap=TTLS'
  'proto=WPA2'
  'anonymous_identity="anonymous@ugent.be"'
  'identity="ndgraef@ugent.be"'
  'ca_cert="/usr/share/ca-certificates/trust-source/mozilla.trust.crt"'
  'phase2="auth=PAP"'
  'password=""'
)

